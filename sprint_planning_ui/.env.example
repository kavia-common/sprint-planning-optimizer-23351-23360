# React app environment variables (build-time)
# These are used by src/api/jiraClient.js and Settings/Jira integration UI.

# Base Jira Cloud URL, e.g., https://your-domain.atlassian.net
REACT_APP_JIRA_BASE_URL=https://your-domain.atlassian.net

# Jira user email (for Basic auth)
REACT_APP_JIRA_EMAIL=your-email@example.com

# Jira API token (Personal Access Token)
REACT_APP_JIRA_API_TOKEN=your-api-token

# OAuth 2.0 (optional, planned for future)
REACT_APP_JIRA_OAUTH_CLIENT_ID=
REACT_APP_JIRA_OAUTH_CLIENT_SECRET=
REACT_APP_JIRA_OAUTH_REDIRECT_URI=http://localhost:3000/oauth/callback

# Optional defaults
REACT_APP_JIRA_DEFAULT_PROJECT_KEY=SPR
